{
  "name": "react-medium-image-zoom",
  "version": "4.3.0",
  "description": "Accessible medium.com-style image zoom for React",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:rpearce/react-medium-image-zoom.git"
  },
  "homepage": "https://github.com/rpearce/react-medium-image-zoom",
  "bugs": "https://github.com/rpearce/react-medium-image-zoom/issues",
  "author": "Robert Pearce <me@robertwpearce.com>",
  "license": "BSD-3-Clause",
  "keywords": [
    "react",
    "medium",
    "image",
    "zoom",
    "image zoom",
    "modal",
    "react-component"
  ],
  "tags": [
    "react",
    "medium",
    "image",
    "zoom",
    "image zoom",
    "modal",
    "react-component"
  ],
  "sideEffects": [
    "dist/**/*.css"
  ],
  "files": [
    "AUTHORS",
    "LICENSE",
    "dist/"
  ],
  "scripts": {
    "build": "run-s clean build:js build:css build:docs",
    "build:css": "./scripts/buildCSS",
    "build:css:watch": "chokidar \"source/**/*.css\" -c \"./scripts/buildCSS\"",
    "build:docs": "build-storybook -o docs",
    "build:js": "run-p build:js:cjs build:js:esm build:js:umd",
    "build:js:cjs": "tsc -p tsconfig.cjs.json",
    "build:js:esm": "tsc -p tsconfig.esm.json",
    "build:js:esm:watch": "tsc -p tsconfig.esm.json -w",
    "build:js:umd": "tsc -p tsconfig.umd.json",
    "ci": "run-p lint build test",
    "clean": "run-p clean:dist clean:docs",
    "clean:dist": "rm -rf dist",
    "clean:docs": "rm -rf docs",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "prepublishOnly": "run-p lint test && run-p build:js build:css",
    "start": "run-s start:init start:run",
    "start:init": "run-s clean:dist build:js:esm build:css",
    "start:run": "run-p build:js:esm:watch build:css:watch storybook",
    "storybook": "start-storybook -p 6006",
    "test": "jest"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "5.3.18",
    "@testing-library/jest-dom": "5.7.0",
    "@testing-library/react": "10.0.4",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.5",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "all-contributors-cli": "6.14.2",
    "axe-core": "3.5.3",
    "babel-loader": "8.1.0",
    "chokidar-cli": "2.1.0",
    "coveralls": "3.1.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jest": "23.10.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "ts-jest": "25.5.1",
    "ts-loader": "7.0.3",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "dependencies": {
    "focus-options-polyfill": "1.2.0",
    "react-use": "14.2.0",
    "tslib": "1.11.2"
  }
}
